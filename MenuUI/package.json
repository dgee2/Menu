{
  "name": "MenuUI",
  "version": "0.0.0",
  "scripts": {
    "dev": "yarn run generate-openapi && vite",
    "build": "yarn run generate-openapi && vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "generate-openapi": "openapi-typescript ./open-api/*.yaml --output ./src/generated/open-api"
  },
  "dependencies": {
    "@mdi/font": "^7.0.0",
    "@qdrant/openapi-typescript-fetch": "^1.2.1",
    "pinia": "^2.1.3",
    "roboto-fontface": "*",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vuetify": "^3.3.3",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.22.5",
    "@types/node": "^18.16.17",
    "@types/webfontloader": "^1.6.35",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.42.0",
    "eslint-plugin-vue": "^9.14.1",
    "openapi-typescript": "^6.2.6",
    "typescript": "^5.1.3",
    "vite": "^4.3.0",
    "vite-plugin-vuetify": "^1.0.0",
    "vue-tsc": "^1.2.0"
  }
}
