{
  "name": "MenuUI",
  "version": "0.0.0",
  "scripts": {
    "dev": "yarn run generate-openapi && vite",
    "build": "yarn run generate-openapi && vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "generate-openapi": "openapi-typescript ./open-api/*.yaml --output ./src/generated/open-api"
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "openapi-typescript-fetch": "^1.1.3",
    "pinia": "^2.0.23",
    "roboto-fontface": "*",
    "vue": "^3.2.0",
    "vue-router": "^4.0.0",
    "vuetify": "^3.0.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.21.4",
    "@types/node": "^18.15.0",
    "@types/webfontloader": "^1.6.35",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "openapi-typescript": "^6.2.4",
    "typescript": "^5.0.0",
    "vite": "^4.2.0",
    "vite-plugin-vuetify": "^1.0.0",
    "vue-tsc": "^1.2.0"
  }
}
